    "moment": "^2.24.0",
    "netlify-cms": "^2.9.7",
    "node-sass": "^4.14.1",
    "normalize-scss": "^7.0.1",
    "prismjs": "^1.17.1",
    "react": "^16.10.2",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "7.6.0",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "@babel/core": "7.6.4",
